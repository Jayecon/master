      local temp = ustrregexrf("`temp'" , "\-"               , "")
      local temp = ustrregexrf("`temp'" , "\([\/가-힣]+\)"   , "")
      local temp = ustrregexrf("`temp'" , "\([\/가-힣]+\)"   , "")
      local temp = ustrregexrf("`temp'" , "\([\/가-힣]+"     , "")
      local temp = ustrregexrf("`temp'" , "[0-9]+"           , "")
      /*과학고 항목*/
      local temp = ustrregexrf("`temp'" , "과고$"            , "과학고등학교")
      local temp = ustrregexrf("`temp'" , "과학$"            , "과학고등학교")
